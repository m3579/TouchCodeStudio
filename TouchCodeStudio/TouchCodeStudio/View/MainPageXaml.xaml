  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
  xmlns:local="clr-namespace:TouchCodeStudio;assembly=TouchCodeStudio"       
  x:Class="TouchCodeStudio.MainPageXaml">
  
  <MasterDetailPage.Master>
    <local:FileExplorerPage x:Name="fileExplorerPage" />  
  </MasterDetailPage.Master>
  
  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <local:EditorPage />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>

</MasterDetailPage>